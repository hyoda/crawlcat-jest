{
  "name": "crawlcat-jest",
  "version": "1.0.0",
  "description": "AI-powered Jest and Puppeteer learning project with natural language scenario generation",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "jest --testPathPattern=e2e",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts lib/**/*.ts ai/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "generate:code": "ts-node scripts/generate-code.ts",
    "ai:generate-scenario": "ts-node ai/scenario-generator/cli.ts",
    "ai:convert-scenario": "ts-node ai/scenario-generator/convert-cli.ts",
    "ai:review-code": "ts-node ai/code-reviewer/cli.ts",
    "ai:expand-testcases": "ts-node ai/test-case-generator/cli.ts",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "clean": "rimraf dist coverage e2e/outputs/generated",
    "setup": "npm install && npm run build"
  },
  "keywords": [
    "jest",
    "puppeteer",
    "testing",
    "crawling",
    "ai",
    "code-generation",
    "e2e",
    "learning"
  ],
  "author": "seohyoda",
  "license": "MIT",
  "devDependencies": {
    "@types/cheerio": "^0.22.31",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^8.57.0",
    "jest-html-reporters": "^3.1.7",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "cheerio": "^1.0.0-rc.12",
    "fs-extra": "^11.1.1",
    "handlebars": "^4.7.8",
    "jest": "^29.7.0",
    "jest-puppeteer": "^9.0.2",
    "openai": "^4.20.1",
    "puppeteer": "^22.8.2",
    "yaml": "^2.3.4"
  }
}
